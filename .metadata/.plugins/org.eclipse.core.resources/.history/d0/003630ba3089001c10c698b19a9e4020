
public class primera_parte {

	public static void main(String[] args) {
		
		int edad;
		double peso;
		char numero;
		boolean real;
		
		edad = 20;
		peso = 70.5;
		numero = 2;
		real = true;
		
		System.out.println("Su edad es " + edad + ", su peso es " + peso + ", su número es el " + numero + ", y su estado es " + real);
		
		double primera;
		double segunda;
		double tercera;
		final double finisher = 1.0;
		
		primera = finisher;
		
		segunda = primera;
		tercera = primera;
		
		System.out.println("La primera es = " + primera + ", la segunda es = " + segunda + ", la tercera es = " + tercera + ", y la última es = " + finisher);
		
	}

}
