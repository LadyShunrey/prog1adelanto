
public class clase2_ejemplo1 {

	public static void main(String[] args) {
		
		//declaración de la variable edad de tipo int, la edad solo puede tomar valores enteros
		
		int edad;
		
		//declaración de las variables altura y peso de tipo double, altura y peso solo pueden tomar valores reales
		
		double altura, peso;
		
		//declaración de las variables existe de tipo boolean, existe solo puede tomar los valores booleanos true y false
		
		boolean existe;
		
		
		
	}

}
